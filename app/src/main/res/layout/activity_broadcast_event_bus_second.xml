<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nanorus.trainproject.broadcast_eventbus.BroadcastEventBusSecondActivity">

    <Button
        android:id="@+id/activity_broadcast_event_bus_second_btn_bind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bind"/>

    <Button
        android:id="@+id/activity_broadcast_event_bus_second_btn_unbind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="unbind"
        app:layout_constraintLeft_toRightOf="@+id/activity_broadcast_event_bus_second_btn_bind"
        tools:layout_editor_absoluteY="0dp"/>
</android.support.constraint.ConstraintLayout>

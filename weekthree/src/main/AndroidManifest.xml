<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.weekthree">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".routes.RoutesActivity">
        </activity>
        <activity android:name=".routes.RouteDetailsActivity">
        </activity>
        <activity android:name=".CustomComponentActivity">
        </activity>
        <activity android:name=".LocationActivity">
        </activity>
        <activity android:name=".AccelerometerActivity">
        </activity>
        <activity android:name=".PreferencesActivity">
        </activity>
        <activity android:name=".CustomViewActivity">
        </activity>
    </application>

</manifest>